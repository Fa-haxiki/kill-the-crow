/* 水平间距 */
canvas {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.start-container {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.start-container image {
  width: 80%;
  height: 16.24vw;
  margin-bottom: 200px;
}
.re-start {
  position: absolute;
  bottom: 50px;
}

